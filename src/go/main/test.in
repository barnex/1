# Example input file
#
# * Characters after a hashmark (#) are ignored.
# * The general syntax is: "command: argument1 argument2 ...".
# * Words may be freely separated by any number of spaces or tabs, and the colon (:) is optional.
# * Commands are case-insensitive
# * Linefeeds may be replaced by semicolons (;)
#

#material parameters

msat:       800E3       # in A/m
aexch:      1.3E-11     # in J/m
alpha:      1.0


#geometry

size:       1     128       32            # number of cells in X,Y,Z
cellsize:   3E-9  3.125e-9  3.125E-9      # cell size in m


#run simulation

dt:         0.1E-12   # time step in seconds
run:        1E-9      # run for 1 ns

