# Spintorque standard problem

maxerror 1e-5
demagaccuracy 11

aexch:  13e-12
msat:   8e5

partsize:  10e-9 100e-9 100e-9
gridsize:  1     64    64

#relax
loadm    relaxed.tensor
#alpha:  1
#run:    1e-9
#save:   m png
#save:   m binary


#run
alpha:            0.1
currentdensity:   0 0 1e12
xi:               0.05
spinpolarization: 1

autosave m     png   100e-12
autosave table ascii 20e-12
run 10e-9
