# Micromagnetic standard problem 4
# but 3D version

solvertype:  	heun
maxerror:       1E-5             

# material
msat:       	800E3       # A/m
aexch:      	1.3E-11     # J/m
alpha:      	0.02

# geometry 
gridsize:       128 32 2
partsize:   	500e-9	125e-9 3e-9

# initial magnetization
load:    	s-state.tensor


# run
autosave:	m	 png	   50E-12
autosave:	table	 ascii	   10E-12
staticfield: 	-24.6E-3 4.3E-3    0
run:          	1E-9                     

