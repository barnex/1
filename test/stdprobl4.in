# Micromagnetic standard problem 4


# hardware selection
 remote 127.0.0.1 2527
#cpu

# output
outputdir:  	stdprobl4
verbosity:  	0

solvertype:  	euler
dt:           	0.02E-12                  # s

# material
msat:       	800E3       # A/m
aexch:      	1.3E-11     # J/m
alpha:      	0.02

# geometry 
size:       	1     32          128    
cellsize:   	3E-9  3.90625E-9  3.90625E-9  # m

# initial magnetization
load:    	s-state.t


# run
autosave:	m	png	20E-12
autosave:	table	ascii	10E-12
appliedfield: 	0     4.3E-3    -24.6E-3  # T
run:          	0.01E-9                      # s

