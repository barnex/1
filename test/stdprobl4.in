# Micromagnetic standard problem 4


# hardware selection
cpu


# output
outputdir:    stdprobl4
verbosity:    2

solvertype:   heun
dt:           0.05E-12                  # s

# material
msat:         800E3       # A/m
aexch:        1.3E-11     # J/m
alpha:        0.02

# geometry
size:         2     32          128
cellsize:     1.5E-9  3.90625E-9  3.90625E-9  # m

# initial magnetization
load:     s-state.t


# run
#autosave:  m png 50E-12
autosave: table ascii 10E-12
appliedfield:   0     4.3E-3    -24.6E-3  # T
run:            0.2E-9                      # s

