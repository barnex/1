# Micromagnetic standard problem 4


solvertype  	heun
dt           	0.01E-12    # s
maxerror	1E-5

# material
msat       	800E3       # A/m
aexch      	1.3E-11     # J/m
alpha      	2

# geometry 
size       	1     32          128    
cellsize   	3E-9  3.90625E-9  3.90625E-9  # m

# initial magnetization
uniform 1 1 1


# run
autosave	m	png	50E-12
autosave	table	ascii	10E-12
autosave  m ascii 50E-12
staticfield 	0     4.3E-3    -24.6E-3  # T
run          	1E-9                      # s
save m binary
