# Micromagnetic standard problem 4


# hardware selection
cpu


# output
outputdir:  	output
#verbosity:  	3

solvertype:  	heun
dt:           	0.15E-12                  # s

# material
msat:       	800E3       # A/m
aexch:      	1.3E-11     # J/m
alpha:      	0.02
demagaccuracy: 1

# geometry 
size:       	1     32          128    
cellsize:   	3E-9  3.90625E-9  3.90625E-9  # m

# initial magnetization
load:    	s-state.t


# run
autosave:	m	png	50E-12
autosave:	table	ascii	10E-12
staticfield: 	0     4.3E-3    -24.6E-3  # T
run:          	1E-9                      # s

