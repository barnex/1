
# hardware selection
cpu


# output
outputdir:  vortex
verbosity:  1


# material parameters
msat:       800E3       # A/m
aexch:      1.3E-11     # J/m    


# geometry
size       1     64           64
cellsize   12.5E-9  10E-9  10E-9


# initial magnetization
vortex    1 1

# relax
save       m png
autosave   m png  10E-12
alpha      1.0
dt         0.5E-12
run        0.1E-9

uniform 1 1 1
save m png
run 0.1E-9

