
# hardware selection
cpu


# output
outputdir:  vortex
verbosity:  1


# material parameters
msat:       800E3       # A/m
aexch:      1.3E-11     # J/m    


# geometry
size       4     64           64
cellsize   12E-9  5E-9  5E-9


# initial magnetization
vortex    1 1

# relax
field      0 10E-3 0
alpha      1.0
save       m png
autosave   m png  100E-12
dt         0.5E-12
run        1E-9

# run
alpha   0.02
field   0 0 0
dt      0.01E-12
autosave m png 10E-12
run     10E-9