
# hardware selection
cpu


# output
outputdir:  vortex
verbosity:  1


# material parameters
msat:       800E3       # A/m
aexch:      1.3E-11     # J/m
alpha:      0.05    


# geometry
size       4     64           64
cellsize   12.5E-9  10E-9  10E-9


# initial magnetization
vortex    1 1
addnoise  10

# relax
save       m png
autosave   m png  100E-12
dt         0.01E-12
run        10E-9


