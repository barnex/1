# Vortex eigenfrequency

outputdir  	eigenfreq

#solver
solvertype  	heun
maxerror 	1E-5

#material
msat:       	800E3 
aexch:      	1.3E-11

#geometry 
size:       	1      128         128    
cellsize:   	50E-9  3.90625E-9  3.90625E-9
demagaccuracy	10

#initial magnetization
load 		relaxed.t


# excite
alpha		2
autosave	table	ascii	 50E-12
autosave	m	png	250E-12
staticfield	0	0	15E-3
run          	10E-9
save		m 	binary

# release
alpha		0.005
staticfield	0 0 0
run             100E-9



